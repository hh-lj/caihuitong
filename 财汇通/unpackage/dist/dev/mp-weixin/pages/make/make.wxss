.content {
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.header {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 999;
  background-color: white;
}
.header .head {
  height: 120rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.header .head .head-l {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.header .head .head-l-l image {
  width: 90rpx;
}
.header .head .head-l-m {
  width: 1rpx;
  height: 56rpx;
  background: #434343;
  margin-left: 26rpx;
}
.header .head .head-l-r text:first-child {
  display: block;
  margin-left: 18rpx;
  font-weight: bold;
  line-height: 30rpx;
  font-size: 30rpx;
  letter-spacing: 1rpx;
  color: #000000;
}
.header .head .head-l-r text:last-child {
  display: block;
  margin-top: 17rpx;
  margin-left: 18rpx;
  line-height: 20rpx;
  font-size: 20rpx;
  color: #666666;
}
.header .head .head-r {
  width: 45rpx;
  height: 30rpx;
}
.header .head .drawer {
  height: 100%;
  opacity: 1;
}
.header .head .drawer-box {
  width: 100%;
  margin-top: 100rpx;
}
.header .head .drawer-item {
  margin: 30rpx auto;
  width: 250rpx;
  height: 50rpx;
  border: 1rpx solid #161616;
  text-align: center;
  line-height: 50rpx;
  border-radius: 25rpx;
}
.header .head .tel {
  margin: 40rpx auto;
  width: 300rpx;
  height: 70rpx;
  color: #ffffff;
  text-align: center;
  line-height: 70rpx;
  border-radius: 35rpx;
  background-color: #FF504D;
}
.page-banner {
  width: 100%;
}
.crumbs {
  width: 100%;
  height: 54rpx;
  border-bottom: 1px dashed #DCDCDC;
}
.crumbs .content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 100%;
}
.crumbs .content .crumbs-bar {
  width: 5rpx;
  height: 12rpx;
  background-color: #FF504D;
  margin-right: 15rpx;
}
.crumbs .content text {
  font-size: 18rpx;
  color: #666;
}
.make-list {
  width: 100%;
  padding-bottom: 20rpx;
}
.make-list .make-item {
  margin-top: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.make-list .make-item text {
  font-size: 28rpx;
  color: #333;
  line-height: 28rpx;
  letter-spacing: 5rpx;
}
.make-list .make-item text .red {
  color: #FF504D;
}
.make-list .make-item input {
  border: 1px solid #DCDCDC;
  width: 530rpx;
  height: 50rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
  font-size: 28rpx;
  border-radius: 10rpx;
}
.make-list .make-submite-container {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  margin-top: 50rpx;
}
.make-list .make-submite-container button {
  border: none;
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 20rpx;
  color: #666;
  letter-spacing: 5rpx;
  background-color: #dedede;
  margin-right: 20rpx;
  margin-left: 0;
}
.make-list .make-submite-container .cur {
  background-color: #FF504D;
  color: #fff;
}

